<table>
	<thead>
		<th>Name</th>
		<th>Created At</th>
		<th>Updated At</th>
	</thead>
	<% @data_models.each do |data_model| %>
	<tr>
		<td><%= data_model.name %></td>
		<td><%= data_model.created_at.to_s(:short) %></td>
		<td><%= data_model.updated_at.to_s(:short) %></td>
	</tr>
	<% end %>
</table>